{
  "name": "@stormstreaming/stormplayer",
  "version": "2.2.0",
  "description": "",
  "private": false,
  "author": "Web-Anatomy s.c. <contact@web-anatomy.com>",
  "main": "dist/iife/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.esm.d.ts",
  "keywords": [
    "stormstreaming",
    "web",
    "video",
    "player",
    "low-latency",
    "live-streaming"
  ],
  "license": "SEE LICENCE IN LICENCE.txt",
  "repository": "https://github.com/StormStreaming/stormplayer-js",
  "scripts": {
    "clear": "rm -rf dist",
    "typecheck": "tsc --noEmit --emitDeclarationOnly false",
    "cdn-upload": "node scripts/cdn-upload.js",
    "dev": "cross-env NODE_ENV=development BABEL_ENV=iifeBundled rollup -c -w",
    "compile:iifeBundled": "cross-env NODE_ENV=production BABEL_ENV=iifeBundled rollup -c rollup.config.js",
    "compile:umdBundled": "cross-env NODE_ENV=production BABEL_ENV=umdBundled rollup -c rollup.config.js",
    "compile:esmBundled": "cross-env NODE_ENV=production BABEL_ENV=esmBundled rollup -c rollup.config.js",
    "compile:amdBundled": "cross-env NODE_ENV=production BABEL_ENV=amdBundled rollup -c rollup.config.js",
    "compile:cjsBundled": "cross-env NODE_ENV=production BABEL_ENV=cjsBundled rollup -c rollup.config.js",
    "build:types": "yarn run tsc --project tsconfig.json && yarn tscpaths -p tsconfig.json -s ./src -o ./dist/types",
    "compile": "npm-run-all -l -p compile:iifeBundled compile:umdBundled compile:esmBundled compile:amdBundled compile:cjsBundled",
    "build": "npm-run-all -l -p clear compile build:types"
  },
  "dependencies": {
    "@stormstreaming/stormlibrary": "^2.2.1"
  },
  "peerDependencies": {},
  "devDependencies": {
    "@babel/core": "^7.15.0",
    "@babel/plugin-syntax-import-meta": "^7.10.4",
    "@babel/plugin-transform-react-jsx": "^7.14.9",
    "@babel/plugin-transform-runtime": "^7.15.0",
    "@babel/preset-env": "^7.15.0",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.15.0",
    "@babel/runtime": "^7.15.3",
    "@babel/runtime-corejs3": "^7.15.3",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^20.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.0.4",
    "@rollup/plugin-replace": "^3.0.0",
    "babel-plugin-root-import": "^6.6.0",
    "cross-env": "^7.0.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.3.2",
    "rollup": "^2.56.2",
    "rollup-plugin-import-css": "^3.0.2",
    "rollup-plugin-livereload": "^2.0.5",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-serve": "^1.1.0",
    "rollup-plugin-styles": "^3.14.1",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-url": "^3.0.1",
    "run-all": "^1.0.1",
    "sass": "^1.45.1",
    "serve": "^11.3.2",
    "tscpaths": "^0.0.9",
    "typescript": "^4.3.5",
    "yarn": "^1.22.17",
    "sftp-upload": "^1.0.3"
  },
  "publishConfig": {
    "access": "public"
  },
  "browserslist": "> 0.25%, not dead"
}
