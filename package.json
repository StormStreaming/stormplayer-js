  "scripts": {
    "clear": "rm -rf dist",
    "typecheck": "tsc --noEmit --emitDeclarationOnly false",
    "lint": "yarn eslint ./src",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "code-check": "yarn typecheck && yarn tests && yarn lint",
    "dev": "cross-env NODE_ENV=development BABEL_ENV=iifeBundled rollup -c -w",
    "build:types": "yarn run tsc --project tsconfig.json && yarn tscpaths -p tsconfig.json -s ./src -o ./dist/types",
    "compile:iifeBundled": "yarn build:types && cross-env NODE_ENV=production BABEL_ENV=iifeBundled rollup -c rollup.config.js",
    "compile:umdBundled": "yarn build:types && cross-env NODE_ENV=production BABEL_ENV=umdBundled rollup -c rollup.config.js",
    "compile:esmBundled": "yarn build:types && cross-env NODE_ENV=production BABEL_ENV=esmBundled rollup -c rollup.config.js",
    "compile:amdBundled": "yarn build:types && cross-env NODE_ENV=production BABEL_ENV=amdBundled rollup -c rollup.config.js",
    "compile:cjsBundled": "yarn build:types && cross-env NODE_ENV=production BABEL_ENV=cjsBundled rollup -c rollup.config.js",
    "compile": "npm-run-all -l compile:iifeBundled compile:umdBundled compile:esmBundled compile:amdBundled compile:cjsBundled",
    "build": "npm-run-all -l clear compile build:types"
  },
